  "scripts": {
    "build": "vite build",
    "serve": "node scripts/serve.js",
    "test": "echo 'No automated tests configured yet' && exit 0",
    "lint": "node scripts/run-lint.js",
    "lint:fix": "node scripts/run-lint.js --fix",
    "syeddy:lint": "eslint .",
    "syeddy:test": "node -e \"require('./ai_pod/syeddy-orchestrator.json')\"",
    "syeddy:regress": "node ./ai_pod/tests/run_regression.js",
    "laptops:test": "node ./ai_pod/tests/laptops.spec.mjs",
    "brand:check": "node ./ai_pod/tests/check_assets_licenses.cjs",
    "sri:inject": "node ./ai_pod/scripts/inject_sri.js",
    "mobile:check": "node ./ai_pod/tests/mobile_check.cjs",
    "syeddy:prune": "node -e \"try{const fs=require('fs');const p='ai_pod/tests/test_run_logs.jsonl';if(fs.existsSync(p)){const l=fs.readFileSync(p,'utf8').trim().split(/\\n/).slice(-5000).join('\\n');fs.writeFileSync(p,l)} }catch(e){}\"",
    "syeddy:ci": "npm run syeddy:lint && npm run syeddy:test && npm run laptops:test && npm run brand:check && npm run mobile:check && npm run playwright:test && npm run syeddy:prune",
    "playwright:test": "playwright test"
  },
